{
  "name": "angular-starter",
  "version": "20.3.10",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "tailwind:dev": "tailwindcss -i ./src/tailwind.css -o ./src/styles.css --watch",
    "tailwind:build": "tailwindcss -i ./src/tailwind.css -o ./src/styles.css --minify",
    "build": "npm run tailwind:build && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "coverage": "ng test --no-watch --code-coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.3.12",
    "@angular/compiler": "20.3.12",
    "@angular/core": "20.3.12",
    "@angular/forms": "20.3.12",
    "@angular/platform-browser": "20.3.12",
    "@angular/router": "20.3.12",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.3.10",
    "@angular/cli": "20.3.10",
    "@angular/compiler-cli": "20.3.12",
    "@types/jasmine": "5.1.13",
    "jasmine-core": "5.12.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.2"
  }
}